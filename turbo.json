{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "dev": {
      "cache": false
    },
    "typecheck": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["typecheck"],
      "cache": false
    }
  },
  "globalEnv": [".env", "PREACT_DEV_API_URL", "DATABASE_URI", "JWT_SECRET", "PORT"]
}
