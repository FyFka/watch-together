{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": [".env"],
  "globalDependencies": ["$PREACT_DEV_API_URL", "$DATABASE_URI", "$JWT_SECRET", "$PORT"]
}
