{
  "name": "watch-together",
  "version": "1.0.0",
  "description": "## CLI Commands *   `npm install`: Installs dependencies",
  "main": "./src/server.ts",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:server": "nodemon ./src/server.ts",
    "dev:client": "cd client && npm run dev",
    "deploy": "tsc && node ./build/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FyFka/watch-together.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FyFka/watch-together/issues"
  },
  "homepage": "https://github.com/FyFka/watch-together#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "socket.io": "^4.5.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.17",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1"
  }
}
